window.datePickerOptions = {
  dateFormat:      'dd MM yy',
  buttonImageOnly: true,
  buttonImage:     '<%= asset_path('calendar.png') %>',
  showOn:          'both',
  changeMonth:     true,
  changeYear:      true,
  yearRange:       'c-40:c+1'
}
$(document).ready ->
  $('.dateinput').datepicker window.datePickerOptions
